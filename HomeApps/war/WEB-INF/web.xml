<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>HomeApps</servlet-name>
		<servlet-class>cn.wagentim.homeapps.servlets.HomeAppsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HomeApps</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>data</servlet-name>
		<servlet-class>cn.wagentim.homeapps.servlets.DataServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>data</servlet-name>
		<url-pattern>/data</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>tele</servlet-name>
		<servlet-class>cn.wagentim.homeapps.servlets.TelefonTarifServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>tele</servlet-name>
		<url-pattern>/tele</url-pattern>
	</servlet-mapping>
</web-app>
